                        <Button mnemonicParsing="false" text="终止" />
                        <Button mnemonicParsing="false" text="复位" />
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="360.0" prefWidth="200.0">
               <children>
                  <ScrollPane layoutX="20.0" prefHeight="360.0" prefWidth="360.0">
                     <content>
                        <VBox prefHeight="400.0" prefWidth="350.0" />
                     </content>
                  </ScrollPane>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
