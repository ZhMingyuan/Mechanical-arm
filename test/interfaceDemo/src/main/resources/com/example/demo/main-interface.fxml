<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.MainInterfaceController">
   <children>
      <VBox>
         <children>
            <AnchorPane prefHeight="120.0" prefWidth="200.0">
               <children>
                  <ChoiceBox prefWidth="45.0" />
                  <ImageView fitHeight="60.0" fitWidth="60.0" layoutX="170.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@WechatIMG47.jpeg" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="120.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER_LEFT" layoutX="20.0" prefHeight="60.0" prefWidth="360.0">
                     <children>
                        <Label prefHeight="30.0" prefWidth="81.0" text="操作模式：">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <ChoiceBox prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="60.0" prefHeight="60.0" prefWidth="360.0" spacing="60.0">
                     <children>
                        <Button mnemonicParsing="false" text="启动" />
